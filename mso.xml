<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">

  <!-- IMPORTANT! Id must be unique for your add-in, if you reuse this manifest ensure that you change this id to a new GUID. -->
  <Id>3d7dab89-8dcd-4917-a89c-34785844caed</Id>

  <!--Version. Updates from the store only get triggered if there is a version change. -->
  <Version>1.0.0</Version>
  <ProviderName>Tino Didriksen Consult</ProviderName>
  <DefaultLocale>da-DK</DefaultLocale>
  <DisplayName DefaultValue="Kukkuniaat" />
  <Description DefaultValue="Kukkuniaat" />
  <IconUrl DefaultValue="https://tinodidriksen.com/spell/kal/gas/1.0.0/imgs/logo-32.png" />

  <SupportUrl DefaultValue="https://tinodidriksen.com/spellers/" />
  <AppDomains>
    <AppDomain>retmig.dk</AppDomain>
    <AppDomain>oqaasileriffik.gl</AppDomain>
    <AppDomain>tinodidriksen.com</AppDomain>
  </AppDomains>

  <Hosts>
    <Host Name="Document" />
  </Hosts>
  <Requirements>
    <Sets DefaultMinVersion="1.3">
      <Set Name="WordApi" MinVersion="1.3"/>
    </Sets>
  </Requirements>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://tinodidriksen.com/spell/kal/gas/1.0.0/html/sidebar.html" />
  </DefaultSettings>

  <Permissions>ReadWriteDocument</Permissions>

  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">

    <Hosts>
      <Host xsi:type="Document">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="TDC.KAL.GetStarted.Title"/>
            <Description resid="TDC.KAL.GetStarted.Description"/>
            <LearnMoreUrl resid="TDC.KAL.GetStarted.LearnMore"/>
          </GetStarted>

          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="TDC.KAL.Group1">
                <Label resid="TDC.KAL.Group1Label" />
                <!-- Icons. Required sizes 16,32,80, optional 20, 24, 40, 48, 64. Strongly recommended to provide all sizes for great UX. -->
                <!-- Use PNG icons. All URLs on the resources section must use HTTPS. -->
                <Icon>
                  <bt:Image size="16" resid="TDC.KAL.logo16" />
                  <bt:Image size="32" resid="TDC.KAL.logo32" />
                  <bt:Image size="80" resid="TDC.KAL.logo80" />
                </Icon>

                <Control xsi:type="Button" id="TDC.KAL.TaskpaneButton">
                  <Label resid="TDC.KAL.TaskpaneButton.Label" />
                  <Supertip>
                    <Title resid="TDC.KAL.TaskpaneButton.Label" />
                    <Description resid="TDC.KAL.TaskpaneButton.Tooltip" />
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="TDC.KAL.logo16" />
                    <bt:Image size="32" resid="TDC.KAL.logo32" />
                    <bt:Image size="80" resid="TDC.KAL.logo80" />
                  </Icon>

                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="TDC.KAL.Taskpane" />
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>

    <Resources>
      <bt:Images>
        <bt:Image id="TDC.KAL.logo16" DefaultValue="https://tinodidriksen.com/spell/kal/gas/1.0.0/imgs/logo-16.png" />
        <bt:Image id="TDC.KAL.logo32" DefaultValue="https://tinodidriksen.com/spell/kal/gas/1.0.0/imgs/logo-32.png" />
        <bt:Image id="TDC.KAL.logo80" DefaultValue="https://tinodidriksen.com/spell/kal/gas/1.0.0/imgs/logo-80.png" />
      </bt:Images>
      <bt:Urls>
        <bt:Url id="TDC.KAL.Taskpane" DefaultValue="https://tinodidriksen.com/spell/kal/gas/1.0.0/html/sidebar.html" />
        <bt:Url id="TDC.KAL.GetStarted.LearnMore" DefaultValue="https://tinodidriksen.com/spellers/" />
      </bt:Urls>
      <!-- ShortStrings max characters==125. -->
      <bt:ShortStrings>
        <bt:String id="TDC.KAL.TaskpaneButton.Label" DefaultValue="Kukkuniaat" />
        <bt:String id="TDC.KAL.Group1Label" DefaultValue="Kukkuniaat" />
        <bt:String id="TDC.KAL.GetStarted.Title" DefaultValue="Kom godt i gang med Kukkuniaat!" />
      </bt:ShortStrings>
      <!-- LongStrings max characters==250. -->
      <bt:LongStrings>
        <bt:String id="TDC.KAL.TaskpaneButton.Tooltip" DefaultValue="Klik for at åbne Kukkuniaat" />
        <bt:String id="TDC.KAL.GetStarted.Description" DefaultValue="Kukkuniaat er nu installeret. Gå til fanen Hjem og klik på Kukkuniaat for at komme i gang." />
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>
